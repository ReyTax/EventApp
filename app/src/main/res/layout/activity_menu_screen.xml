<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_menu_screen_drawerLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".menuscreen.MenuScreenActivity"
    tools:openDrawer="end"
    android:background="@color/green">





            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/activity_menu_screen_toolbar"
                app:layout_constraintVertical_bias="0.0">


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_shape"
                    app:layout_constraintTop_toTopOf="parent">


                        <androidx.gridlayout.widget.GridLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:alignmentMode="alignMargins"
                            app:columnCount="2"
                            app:columnOrderPreserved="false"
                            app:rowCount="3"
                            app:useDefaultMargins="false">


                            <androidx.cardview.widget.CardView
                                android:id="@+id/activity_menu_screen_cardViewAddEvent"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_margin="13dp"
                                app:cardCornerRadius="8dp"
                                app:cardElevation="12dp"
                                app:layout_column="0"
                                app:layout_columnWeight="1"
                                app:layout_row="0"
                                app:layout_rowWeight="1">


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/activity_menu_screen_imageViewAddEvent"
                                        android:layout_width="100dp"
                                        android:layout_height="100dp"
                                        app:srcCompat="@drawable/ic_post_add" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/text_add_event"
                                        android:textSize="18sp" />

                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:id="@+id/qwe"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_margin="13dp"
                                app:cardCornerRadius="8dp"
                                app:cardElevation="12dp"
                                app:layout_column="1"
                                app:layout_columnWeight="1"
                                app:layout_row="0"
                                app:layout_rowWeight="1">


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/qwee"
                                        android:layout_width="100dp"
                                        android:layout_height="100dp"
                                        app:srcCompat="@drawable/ic_post_add" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/text_add_event"
                                        android:textSize="18sp" />

                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:id="@+id/qweee"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_margin="13dp"
                                app:cardCornerRadius="8dp"
                                app:cardElevation="12dp"
                                app:layout_column="0"
                                app:layout_columnWeight="1"
                                app:layout_row="1"
                                app:layout_rowWeight="1">


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/qweeee"
                                        android:layout_width="100dp"
                                        android:layout_height="100dp"
                                        app:srcCompat="@drawable/ic_post_add" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/text_add_event"
                                        android:textSize="18sp" />

                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:id="@+id/qweeeeee"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_margin="13dp"
                                app:cardCornerRadius="8dp"
                                app:cardElevation="12dp"
                                app:layout_column="1"
                                app:layout_columnWeight="1"
                                app:layout_row="1"
                                app:layout_rowWeight="1">


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/qweeeeeee"
                                        android:layout_width="100dp"
                                        android:layout_height="100dp"
                                        app:srcCompat="@drawable/ic_post_add" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/text_add_event"
                                        android:textSize="18sp" />

                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_margin="26dp"
                                app:cardCornerRadius="8dp"
                                app:cardElevation="8dp"
                                app:layout_column="0"
                                app:layout_columnWeight="1"
                                app:layout_row="2"
                                app:layout_rowWeight="1">


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/imagerweeView"
                                        android:layout_width="80dp"
                                        android:layout_height="80dp"
                                        app:srcCompat="@drawable/ic_post_add" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="Add Event" />
                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_margin="26dp"
                                app:cardCornerRadius="8dp"
                                app:cardElevation="8dp"
                                app:layout_column="1"
                                app:layout_columnWeight="1"
                                app:layout_row="2"
                                app:layout_rowWeight="1">


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/yterwerwerry"
                                        android:layout_width="80dp"
                                        android:layout_height="80dp"
                                        app:srcCompat="@drawable/ic_post_add" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="Add Event" />
                                </LinearLayout>
                            </androidx.cardview.widget.CardView>
                        </androidx.gridlayout.widget.GridLayout>

                </RelativeLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.drawerlayout.widget.DrawerLayout>
